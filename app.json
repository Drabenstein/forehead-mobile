{
  "expo": {
    "name": "forehead-mobile",
    "slug": "forehead-mobile",
    "version": "1.0.0",
    "orientation": "landscape",
    "backgroundColor": "#4ab7ff",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "androidStatusBar": {
      "hidden": true,
      "translucent": true
    },
    "androidNavigationBar": {
      "visible": "sticky-immersive",
      "barStyle": "light-content"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "657c2339-7bb9-4316-b27c-15baf0906fa7"
      }
    },
    "owner": "drabenstein"
  }
}
